{
  "$schema": "https://raw.githubusercontent.com/Stremio/stremio-web/development/themes/schema.json",
  "id": "com.github.sic-em.stremio-theme-catppuccin",
  "name": "Catppuccin",
  "version": "1.0.0",
  "description": "Soothing pastel theme for Stremio Web. Four flavors: Latte (light), Frappé, Macchiato, and Mocha (dark).",
  "author": "sic-em",
  "homepage": "https://github.com/sic-em/stremio-theme-catppuccin",
  "preview": "flavors/mocha/preview.png",
  "flavors": [
    {
      "id": "latte",
      "name": "Latte",
      "isDark": false,
      "css": "flavors/latte/theme.css",
      "preview": "flavors/latte/preview.png"
    },
    {
      "id": "frappe",
      "name": "Frappé",
      "isDark": true,
      "css": "flavors/frappe/theme.css",
      "preview": "flavors/frappe/preview.png"
    },
    {
      "id": "macchiato",
      "name": "Macchiato",
      "isDark": true,
      "css": "flavors/macchiato/theme.css",
      "preview": "flavors/macchiato/preview.png"
    },
    {
      "id": "mocha",
      "name": "Mocha",
      "isDark": true,
      "css": "flavors/mocha/theme.css",
      "preview": "flavors/mocha/preview.png"
    }
  ]
}
